var args = arguments[0] || {};
var id = args.house_id || {};

Alloy.Collections.house.fetch();

function showHouse(collection) {
	return collection.where({id: parseInt(id)});
}

function showLocation() {
	var houseLocationController = Alloy.createController("houseLocation", {
			estate_name: e.annotation.Name
	});
	
	Alloy.Globals.index.activeTab.open(houseLocationController.getView());
}
